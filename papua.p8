function _init()
	poke(0x5f2d,1)
	cur={
		x=stat(32),
		y=stat(33),
		sp=1,
		bt=stat(34),
		rel=false
	}
	xoff={1,-1,0,0}
	yoff={0,0,1,-1}
	init_armies()
	players={}
	opponents={}
	
	t1x=8
	t1y=30
	
	tt1x=8
	tt1y=30
	
	tmp_table=nil
	tmp_index=nil
	
	_upd=upd_title
	_drw=drw_title
	
	t=0
--	load_r5(opponents)	
	load_army(players,"glo")
	load_army(players,"kha")
	load_army(players,"oss")
	load_army(players,"sou")
	load_army(players,"tze")
	
	init_stars()
	init_comet()
end

function _update()
	t+=1
	upd_cur()
	_upd()
end

function _draw()
	cls()
	_drw()
	drw_cur()
end

function upd_match()
	t1x=lerp(t1x,tt1x,.2)
	t1y=lerp(t1y,tt1y,.2)
	upd_stars()
	upd_comet()
end

function drw_match()
	drw_stars()
	drw_comet()
	drw_osc_spr(22,110,6,2,2)
	map()
	wobble(16,0,8,8,104,104,16,16)
	drw_columns()
	drw_rows()
	drw_all()
end

function upd_select()
		
end

function drw_select()
	
end

function upd_title()
	if cur.bt==1 then
		_drw=drw_match
		_upd=upd_match
	end
end

function drw_title()
	title_screen()
end
