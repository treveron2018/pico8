function _init()
	poke(0x5f2d,1)
	cartdata("trev_asteroidont_1_5")
	dpal={0,1,1,2,1,13,6,4,4,9,3,13,1,13,14}	
	cur={
		x=64,
		y=64,
		bt=0,
		spt=1,
		tax=0,
		tay=0
	}
	h_score=dget(0)
	if (not h_score)h_score=0
	btn_released=true
	fadeperc=1
	t=0
	player_sel={}
	load_player_n()
	players=2

--power up variables
	maxdir_timer=30
	dir_timer=maxdir_timer
	ast_spd=1.5
	maxaspawn_t=600
	aspawn_t=0
	amp_r=0
	ast_div=2
	cmaxr=2
	ast_hp=1
		
	sposx=0
	sposy=0
	spw_sw=false
		
--debuffs for ships
	base_sspawn_t=180
	maxsspawn_t=180
	sspawn_t=0
	shoot_t=0
	shoot_tl=120
	bullet_sl=1
	bulspddiv=2

	dir=false
	p2_t=0
	lvl=1
	xp=0
	next_lvl=3
	score=0
	start_title()
end

function _update60()
	t+=1
	upd_cursor()
	_upd()
end

function start_title()
	p=nil
	shwaves={}
	asteroids={}
	ships={}	
	spawn_ast(10,10)
	if(asteroids[1].dx<0)asteroids[1].dx=.25
	p=spawn_ship(64,104)
	_upd=upd_title
	_drw=drw_title
end

function start_game()

	music(0,0,12)
	t=0
	over=false
	over_t=0
	ufo={}
	bullets={}
	sparks={}		
	
	power_ups={}
	pow_n=2
	pow_id={1,2,3,4,5,6,7,8,9}
	pow_lvl={1,1,1,1,1,1,2,2,2}
	pow_txt={"faster asteroids",
		"slower bullets",
		"⬆️ asteroid spawn",
		"⬆️ direction time",
		"⬆️asteroid radius",
		"more options",
		"⬆️ asteroid hp",
		"⬆️ asteroid div",
		"⬆️asteroid number"
	}
	pow_spr={2,3,4,5,6,7,18,19,20}
		
	ast_spawn_loc={
		{x=-(8+amp_r),y=nil},
		{x=127+(8+amp_r),y=nil},
		{x=nil,y=-(8+amp_r)},
		{x=nil,y=127+(8+amp_r)}
	}
	
	fadeperc=1
	_upd=upd_game
	_drw=drw_game
	
	asteroids={}
	ships={}
	spawn_ast(10,10)
	spawn_ship(64,64)
end

function upd_title()
	cursor_select(player_sel,48,8)
	upd_ast()
	ast_select()
	upd_ships()
	upd_shwaves()
	upd_p2()
	for p in all(player_sel) do
		if cur.bt==1 and btn_released and p.sel then
			players=p.id
			start_game()
		end
	end	
end

function upd_game()
	if(players==2 and #ships>0)upd_p2()
	upd_ast()
	ast_select()
	upd_ships()
	upd_ufo()
	spawn_ufo()
	upd_shwaves()
	upd_sparks()
	upd_bullets()
	if(count_maxr()<cmaxr)spawn_timer()
	spawn_ship_timer()
	check_xp()
	if(over)game_over()
end

function upd_dir()
	dir_timer-=1
	stir(get_ast())
	if cur.bt!=1 or dir_timer<=0 then
		_upd=upd_game
		_drw=drw_game
		dir=false
		sfx(21)
	end
end

function upd_over()
	if cur.bt==1 and btn_released then
		btn_released=false
		start_title()
	end
end

function drw_game()
	drw_elements()
end

function drw_dir()
	drw_elements()
	line(cur.x,cur.y,cur.tax,cur.tay,7)
	show_timer()
end

function drw_title()
	local ast,shp=asteroids[1],ships[1]
	map()
	print("by @trevvieaamb",2,2,7)
	print("hi-score: "..h_score,38,75,7)
	print("click and drag",ast.x+10,ast.y+10,6)
	print("   to stir!",ast.x+10,ast.y+17,6)
	print("⬆️⬇️⬅️➡️ to",shp.x+5,shp.y+5,6)
	print("control p2",shp.x+5,shp.y+11,6)

	drw_pl_sel()
	drw_ast()
	drw_ships()
	drw_shwaves()
end

function drw_over()
	map(16,0)	
	if score>h_score then
		print("new high score!",32,74,7)
		h_score=score
		dset(0,h_score)
	end
	print("score: "..score,32,84,7)	
	mouse_icon(32,100,"back to title",7)
end

function drw_elements()
	drw_ufo()
	drw_ast()
	drw_ships()
	drw_shwaves()
	drw_sparks()
	drw_bullets()
	display_xp()
end

function _draw()
	cls()
	_drw()
	drw_cursor()
	checkfade()
end

