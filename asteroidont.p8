--cursor

function upd_cursor()
	cur.x=stat(32)
	cur.y=stat(33)
	cur.bt=stat(34)
	if(cur.bt==0)btn_released=true
	if(not dir)ast_clic()
end

function drw_cursor()
	spr(cur.spt, cur.x, cur.y)
end

function ast_select()
	for a in all(asteroids)do
		if dist(cur.x,cur.y,a.x,a.y)<a.sr+amp_r then
			a.sel=true
		else
			a.sel=false
		end
	end 
end

function ast_clic()
	for a in all(asteroids)do
		if cur.bt==1 and _upd!=upd_title and a.sel and dir_timer==maxdir_timer then
			cur.tax,cur.tay=a.x,a.y
			dir=true
			_upd=upd_dir
			_drw=drw_dir
			sfx(20)
		elseif cur.bt==0 then
			dir_timer=maxdir_timer
		end
	end
end

function cursor_select(items,w,h)
	for i in all(items)do
		if cur.x<i.x
		or cur.x>i.x+w 
		or cur.y<i.y
		or cur.y>i.y+h then
			i.sel=false
			i.t=0
		else
			i.sel=true
		end
	end
end
