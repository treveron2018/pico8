pico-8 cartridge // http://www.pico-8.com
version 41
__lua__
function _init()
	cx=63
	cy=63
	speed=3
	flipy=false
end

function _update()
	move()
end

function _draw()
	cls(1)
	spr(1,cx,cy,1,1,false,flipy)
end

-->8
--player control

function move()
	if btn(⬅️) then
		cx-=speed
	end
	if btn(➡️) then
		cx+=speed
	end
	if btn(⬆️) then
		cy-=speed
		flipy=false
	end
	if btn(⬇️) then
		cy+=speed
		flipy=true
	end
	snake()
end

function snake()
	if cx>127 then
		cx=-7
	elseif cx+7<0 then
		cx=127
	end
end
