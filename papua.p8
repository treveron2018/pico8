--papua lights
--by treveron

function _init()
	poke(0x5f2d,1)
	cur={
		x=stat(32),
		y=stat(33),
		sp=1,
		bt=stat(34),
		rel=true	
	}
	xoff={1,-1,0,0}
	yoff={0,0,1,-1}
	init_armies()
	players={}
	opponents={}
	
	tmp_table=nil
	tmp_index=nil
	
	_upd=upd_match
	_drw=drw_match
	
	load_r5(opponents)	
	load_army(players,"glo")
	load_army(players,"kha")
	
end

function _update()
	upd_cur()
	_upd()
end

function _draw()
	cls(1)
	_drw()
	drw_cur()
	--print(tmp_table,0,0,7)
	--print(tmp_index,0,6,7)
end

function upd_match()

end

function drw_match()
	drw_columns()
	drw_rows()
end

function upd_select()
	
end

function drw_select()
	drw_all()
end
