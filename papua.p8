--cursor

function upd_cur()	
	cur.x=stat(32)
	cur.y=stat(33)
	if cur.bt==1 then
		cur.rel=false
	else 
		cur.rel=true
	end
	cur.bt=stat(34)	
end

function drw_cur()
	outline(cur)
end

function cur_butt(x1,y1,x2,y2)
	if cur.x<=x1
	or cur.x>=x2
	or cur.y<=y1
	or cur.y>=y2
	then return false
	else	return true
	end
end
