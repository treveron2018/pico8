--tables

function load_all(table)
	for a in all(armies) do
		add(table,a)
	end
end

--loads 5 armies randomly 
--for testing

function load_r5(table)
	for i=1,5 do
		local a=rnd(armies)
		add(table,a)
	end
end

function load_army(table,army)
	for a in all(armies) do
		if(a.name==army)add(table,a)
	end
end

function del_army(table,army)
	for a in all(table) do
		if(a.name==name)del(table,a)
	end
end

function drw_columns()
	local x,y,mx,my=0,0,30,8
	for o in all(opponents) do
		draw_cell(o,x,y,mx,my,opponents)
		x+=#o.name*6
	end
end

function drw_rows()
	local x,y,mx,my=0,0,12,20
	for p in all(players) do
		local name=p.name
		draw_cell(p,x,y,mx,my,players)
		--
		local x2=x+#name*6
		for o in all(opponents) do
			rect(x2-4+mx,y-4+my,x2+#name*4+2+mx,y+8+my,7)
			rectfill(x2-3+mx,y-3+my,x2+#name*4+1+mx,y+7+my,match(p,o))
			x2+=#name*6
		end
		--
		y+=12
	end	
end

function match(p,o)
	local i=get_index(o,armies)
	return p.vs[i]
end

function get_index(o,table)
	for i=1, #table do
		if(o.name==table[i].name)return i
	end
	return false
end

function drw_all()
	local x,y,mx,my,i=0,0,24,15,1
	for a in all(armies) do
		draw_cell(a,x,y,mx,my,armies)
		if i%5==0 then
			x=0
			y+=12
		else
			x+=#a.name*6
		end	
		i+=1
	end
end
