--tools

function outline(o)
	for i=1,#xoff do
		for i=1,16 do
			pal(i,1)
		end
		spr(o.sp,o.x+xoff[i],o.y+yoff[i])
	end
	pal()
	spr(o.sp,o.x,o.y)
end

function draw_cell(o,x,y,mx,my,table)
		local name,col=o.name,7
		local x1,y1,x2,y2=x-4+mx,y-4+my,x+#name*4+2+mx,y+8+my		
		rect(x1,y1,x2,y2,7)
		if cur_butt(x1,y1,x2,y2) then
			rectfill(x1+1,y1+1,x2-1,y2-1,9)
			col=0	
			if cur.bt==1 and cur.rel then 
				if _drw==drw_match then
					_drw=drw_select
					_upd=upd_select
				
					tmp_table=table
					tmp_index=get_index(o,table)
				elseif _drw==drw_select then
					_drw=drw_match
					_upd=upd_match
					del(tmp_table, tmp_table[tmp_index])
					add(tmp_table, o, tmp_index)
					tmp_table, tmp_index=nil,nil			
				end
			end
		end
		print(name,mx+x,my+y,col)
end
