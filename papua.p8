--tools

function outline(o)
	for i=1,#xoff do
		for i=1,16 do
			pal(i,1)
		end
		spr(o.sp,o.x+xoff[i],o.y+yoff[i])
	end
	pal()
	spr(o.sp,o.x,o.y)
end

function draw_cell(o,x,y,mx,my,table)
		local name=o and o.name or " + "
		local col,x1,y1,x2,y2=7,x-4+mx,y-4+my,x+#name*4+2+mx,y+8+my		
		rectfill(x1,y1,x2,y2,0)		
		rect(x1,y1,x2,y2,7)		
		if o and tmp_table and is_rep(o,tmp_table) and tt1x==-120 then
			rectfill(x1+1,y1+1,x2-1,y2-1,0)
			col=5					
		elseif cur_butt(x1,y1,x2,y2) then
			rectfill(x1+1,y1+1,x2-1,y2-1,9)
			col=0	
			if click() then 
				if tt1x==8 then
					tt1x=-120
					tmp_table=table
					tmp_index=o and get_index(o,table) or nil
				elseif tt1x==-120 then
					tt1x=8
					if name!=" ⬅️ " then
						if tmp_index then 
							del(tmp_table, tmp_table[tmp_index])
							if(name!=" ❎ ")add(tmp_table, o, tmp_index)
						else 
							add(tmp_table, o)					
						end
					end
					tmp_table, tmp_index=nil,nil			
				end
			end
		end
		local off = (name==" ❎ " or name==" ⬅️ ") and 2 or 0
		print(name,mx+x-off,my+y,col)
end

function lerp(from, to, weight)
 local dist = to - from
 if (abs(dist) < 0.2) then
  return to
 end
 return from + (dist * weight)
end

function is_rep(army, table)
	for a in all(table) do
		if (a.name==army.name)return true
	end
	return false
end

function drw_osc_spr(sp,x,y,w,h)
	local offy=sin(t/80)*2
	if(offy==2)offy=1.9
	spr(sp,x,y+offy,w,h)
end


function wobble(sx,sy,sw,sh,x,y,w,h)
	local wob=sin(t/60)*w/6
	ox,oy=-wob/2,wob
	ow,oh=wob,wob*-1
	sspr(sx,sy,sw,sh,x+ox,y+oy,w+ow,h+oh)
end
