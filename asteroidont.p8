--gameplay

function check_xp()
	if xp>=next_lvl then
		lvl+=1
		xp=next_lvl-xp
		next_lvl*=1.5
		load_pows()
		_upd=upd_lvl
		_drw=drw_lvl
		maxsspawn_t*=.9
		sfx(22)
	end
end

function upd_lvl()
	cursor_select(power_ups,80,7)

	if cur.bt==1 and get_selpow() and get_selpow().t==60 then
		_upd=upd_game
		_drw=drw_game
		level_up(get_selpow().id)
		power_ups={}
		sfx(23)
	elseif cur.bt==1 then
		sfx(24)
	end
end

function drw_lvl()
	drw_elements()
	local maxi=pow_n
	if (lvl%5==0)maxi=3
	local windx,windy,windx2,windy2=35,30,115,35+maxi*10+10
	rect(windx-1,windy-1,windx2+1,windy2+1,7)
	rectfill(windx,windy,windx2,windy2,0)
	print("level "..lvl.."!",windx+2,windy+2,7)
	for i=1,maxi do
		local _x,_y=2+windx,windy+12*i
		local col=7
		if power_ups[i].sel then
			col=10
			if cur.bt==1 then
				power_ups[i].t+=1
				loading_bar(power_ups[i].t,60,power_ups[i].x-5,power_ups[i].y+1)			
			end
		end
		rect(_x-1,_y-1,_x+8,_y+8,col)
		spr(power_ups[i].spt,_x,_y)
		print(power_ups[i].txt,_x+10,_y+2,col)		
		power_ups[i].x,power_ups[i].y=_x,_y
	end
end

function load_pows()
	local pups,maxi={1,2,3,4,5,6},pow_n
	if lvl%5==0 then
		pups={7,8,9}
		maxi=3
	end
	for i=1,maxi do
		local n=rnd(pups)
		while pow_n==4 and n==6 do
			n=rnd(pups)
		end
		local p={
			id=pow_id[n],
			txt=pow_txt[n],
			spt=pow_spr[n],
			sel=false,
			x=0,
			y=0,
			t=0
		}
		add(power_ups,p)
		del(pups,n)
	end
end

function get_selpow()
	for p in all(power_ups) do
		if(p.sel)return p
	end
	return false
end

function level_up(id)
	if id==1 then
		ast_spd*=1.1
	elseif id==2 then
		bulspddiv*=1.1
	elseif id==3 then
		maxaspawn_t*=.9
	elseif id==4 then
		maxdir_timer*=1.1
	elseif id==5 then
		amp_r+=.5
	elseif id==6 then
		pow_n+=1
	elseif id==7 then
		ast_hp+=1
	elseif id==8 then
		ast_div+=1
	elseif id==9 then
		cmaxr+=1
	end
end

function checkend()
	if #asteroids==0 and not over then
		over=true
		over_t=120
		music(-1,2000)
	end
end

function game_over()
	over_t-=1
	if over_t==0 then
		if(not cur.bt==0)btn_released=false
		_upd=upd_over
		_drw=drw_over
		fadeout(0.02)
	end
end
