function load_all(table)
	for a in all(armies) do
		add(table,a)
	end
end

--loads 5 armies randomly 
--for testing
function load_r5(table)
	for i=1,5 do
		local a=rnd(armies)
		add(table,a)
	end
end

function load_army(table,army)
	for a in all(armies) do
		if(a.name==army)add(table,a)
	end
end

function del_army(table,army)
	for a in all(table) do
		if(a.name==name)del(table,a)
	end
end

function drw_columns()
	local x,y,mx,my=0,0,t1x+22,t1y+4
	for o in all(opponents) do
		draw_cell(o,x,y,mx,my,opponents)
		x+=#o.name*6
	end
	if(#opponents<5)draw_cell(nil,x,y,mx,my,opponents)
end

function drw_rows()
	local x,y,mx,my=0,0,t1x+4,t1y+16
	for p in all(players) do
		local name=p.name
		draw_cell(p,x,y,mx,my,players)
		--
		local x2=x+#name*6
		for o in all(opponents) do
			local mcol,bcol=match(p,o),7
			local _x1,_y1,_x2,_y2=x2-4+mx,y-4+my,x2+#name*4+2+mx,y+8+my
			if cur_butt(_x1,_y1,_x2,_y2) then
				bcol=1
				if cur.bt==1 and cur.rel then
					change_color(p,o)
				end
			end
			rect(_x1,_y1,_x2,_y2,bcol)
			rectfill(_x1+1,_y1+1,_x2-1,_y2-1,mcol)
			x2+=#name*6
		end
		--
		y+=12
	end	
	if(#players<5)draw_cell(nil,x,y,mx,my,players)
end

function match(p,o)
	local i=get_index(o,armies)
	return p.vs[i]
end

function change_color(p,o)
	local col,i=match(p,o),get_index(o,armies)
	if col==8 then
		p.vs[i]=10	
	elseif col==10 then
		p.vs[i]=11
	else
		p.vs[i]=8
	end
end

function get_index(o,table)
	for i=1, #table do
		if(o.name==table[i].name)return i
	end
	return false
end

function drw_all()
	local x,y,mx,my,i=0,0,t1x+16+128,t1y+11,1
	for a in all(armies) do
		draw_cell(a,x,y,mx,my,armies)
		if i%5==0 then
			x=0
			y+=12
		else
			x+=#a.name*6
		end	
		i+=1
	end
	draw_cell({name=" ⬅️ "},x,y,mx,my)
	if tmp_index then
		x+=18
		draw_cell({name=" ❎ "},x,y,mx,my)
	end
end
