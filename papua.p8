--particles

stars={}
trail={}



function init_comet()
	comet={
		x=-10,
		y=5,
		dx=1,
		dy=.1,
		r=5,
		active=false,
		t=0
	}
end

function upd_comet()
	if comet.active then
		comet.x+=comet.dx
		comet.y+=comet.dy
		if(comet.x>=128+comet.r)init_comet()
		local _dy=.2
		for i=1,2 do
			local t={
				x=comet.x,
				y=comet.y,
				r=comet.r+5,
				dy=_dy
			}
			add(trail,t)
			_dy*=-1
		end
	else
		comet.t+=1
		if (comet.t>=300)comet.active=true
	end
	
	for t in all(trail) do
		t.y+=t.dy
		t.r-=.1
		if(t.r<=0)del(trail,t)
	end
end

function drw_comet()
	for t in all(trail) do
		circfill(t.x,t.y,t.r,12)
	end	
	circfill(comet.x,comet.y,comet.r,7)
end

function init_stars()
	for i=1,100 do
		local s={
			x=rnd(128),
			y=rnd(120),
			sx=rnd(2)
		}
		add(stars,s)
	end
end

function upd_stars()
	for s in all(stars) do
		s.x+=s.sx
		if(s.x>128)s.x=0
	end
end

function drw_stars()
	for s in all(stars) do
		local col=0
		if s.sx>1.5 then
			col=7
		elseif s.sx>1 then
			col=6
		elseif s.sx>0.5 then
			col=5
		else col=1
		end
		pset(s.x,s.y,col)
	end
end
