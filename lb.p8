__lua__
--lightbringer
--by treveron

--bug 
-- hb update works wrong when
-- using dx 
function _init()
	t=0
	p={
		x=64,
		y=64,
		dx=0,
		dy=0,
		flp=false,
		i_ani={1},
		w_ani=split("33,34,35,36")
		}
end


function _update60()
	t+=1
	move_player()
end


function _draw()
	cls(12)
	map()
	drw_player()
	
	--debug

	print(debug,0,0,7)
end
-->8
--player

function move_player()
	upd_hb()	
	if btn(➡️) then 
		if(not tilecol_sde(p_fnt_hb))p.dx=1
		p.flp=false
	elseif btn(⬅️)then
		if(not tilecol_sde(p_fnt_hb))p.dx=-1
		p.flp=true
	else 
		p.dx=0
	end
	p.dy=tilecol_dwn(p_flr_hb) and 0 or 1
	p.x+=p.dx
	p.y+=p.dy	
end

function drw_player()
	--offset values depend on wing spr
	local offset1= p.flp and -3 or -5
	local offset2= p.flp and 4 or 2

	spr(2,p.x+offset1,p.y,1,2)
	spr(2,p.x+offset2,p.y,1,2,true)
	
	--spr(3,p.x-14,p.y+2,2,1)
	--spr(3,p.x+3,p.y+2,2,1,true)	
	local frm=p.dx==0 and p.i_ani or p.w_ani
	spr(getspr(frm),p.x,p.y,1,2, p.flp)
end	

function upd_hb()
	p_flr_hb={
	x1=p.flp and p.x+5 or p.x+1,
	y1=p.y+14,
	x2=p.flp and p.x+7 or p.x+3}

	p_fnt_hb={
	x1=p.flp and p.x+3 or p.x+4,
	y1=p.y,
	y2=p.y+13}
end
-->8
--tools

function tilecol_dwn(hb)
	for i=hb.x1, hb.x2 do
		debug=hb.y1
		if(fget(mget(i/8,hb.y1/8),0))return true
	end
	return false
end

function tilecol_sde(hb)
	for i=hb.y1, hb.y2 do
		debug=hb.y1
		if(fget(mget(hb.x1/8,i/8),0))return true
	end
	return false
end

function getspr(ani)
	return ani[flr((t/10%#ani)+1)]
end
